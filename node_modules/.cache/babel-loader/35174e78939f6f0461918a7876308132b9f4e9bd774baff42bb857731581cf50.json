aket from \"@/components/CardPaket.vue\";\nimport axios from \"axios\";\n\nexport default {\n  name: \"HomeView\",\n  components: {\n    Navbar,\n    Hero,\n    CardPaket,\n  },\n\n  data() {\n    return {\n      pakets: [],\n    };\n  },\n  methods: {\n    setBestPakets(data) {\n      this.pakets = data;\n    },\n  },\n  mounted() {\n    axios\n      .get(\"http://localhost:3000/best-pakets\")\n      .then((response) => this.setBestPakets(response.data))\n      .catch((error) => console.log(\"Gagal :\", error));\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}